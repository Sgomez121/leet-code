class Solution {
    public int[] twoSum(int[] nums, int target) {
        Map<int,int> answer=new Hashmap<>();
        for(i=0; i<nums.size();i++){
            for(j=i+1;j<nums.size();j++){
                if(nums[i]+nums[j]==target){
                    answer.put(i,j);
                }
            }
        }
        System.out.println("These are the indeddes that equal to "+ target + answer);
    }
}
